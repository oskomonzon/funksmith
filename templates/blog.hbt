<!DOCTYPE html>
<html lang="es">
  {{> head}}
<body>
  {{> header}}
  <div class="container markdown">
    {{{ contents }}}
    {{#each collections.posts}}
      <article>
        <h2>{{ this.title }}</h2>
        {{{ this.contents }}}
      </article>
    {{/each}}
  </div>
  {{> footer}}
</body>
</html>